<h1>Listings</h1>

<p><%= link_to "Create New", new_article_path, :class=> 'button' %></p>



<table>
    <tr>
         <th>Title</th>
         <th>Description</th>
         <th> </th>  <th> </th>
    </tr>
    <% @list.each do |article| %>
   <tr>
        <td><%= article.title %></td>
        <td><%= article.description %></td>
        <td><%= link_to 'Edit', edit_article_path(article), :class=>'link'  %></td>
        <td><%= link_to 'Show', article_path(article), :class=>'link'   %></td> 
        </td>
    </tr>
    <% end %>
</table>
        